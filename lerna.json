{
  "packages": [
    "packages/*"
  ],
  "version": "independent",
  "ignoreChanges": [
    "**/__snapshots__/**",
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/*.spec.*",
    "**/*.e2e.*",
    "**/*.md"
  ],
  "command": {
    "version": {
      "conventionalCommits": true,
      "yes": true
    },
    "publish": {
      "conventionalCommits": true,
      "yes": true
    }
  }
}
